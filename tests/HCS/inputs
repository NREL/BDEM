#geometry parameters
bdem.prob_lo        = 0.      0.     0.      # lo corner of physical domain
bdem.prob_hi        = 0.0064  0.0064 0.0064  # hi corner of physical domain
bdem.ncells         = 32     32     32 
bdem.max_grid_size  = 8
bdem.is_it_periodic = 1      1      1 

#0 - outflow
#1 - wall
bdem.bc_lower = 0 0 0
bdem.bc_upper = 0 0 0

#timestepping
bdem.final_time=0.5
bdem.max_steps=100000000
bdem.write_output_time=0.01
bdem.num_redist = 25
bdem.cfl = 0.02
bdem.init_particles_using_file = 1

#particle intrinsics
bdem.normal_spring_constant = 10
bdem.normal_restitution_coefficient = 0.8
bdem.friction_coefficient = 0.0
bdem.tangential_spring_constant = 0.1
bdem.tangential_restitution_coefficient = 0.8
bdem.rolling_friction_coefficient = 0.0

bdem.gravity = 0.0 0.0 0.0

bdem.kind_of_geometry=all_regular
bdem.refine_level_set=1
