#geometry parameters
bdem.prob_lo        = 0.   -0.2     0.      # lo corner of physical domain
bdem.prob_hi        = 0.6   0.6    0.6     # hi corner of physical domain
bdem.ncells         = 4      8     4
bdem.max_grid_size  = 4
bdem.is_it_periodic = 0      0      0

#0 - outflow
#1 - wall
bdem.bc_lower = 0 1 0
bdem.bc_upper = 0 2 0

#timestepping
bdem.final_time=0.5
bdem.max_steps=100000000
bdem.write_output_time=0.01
bdem.num_redist = 2
bdem.cfl = 0.02
bdem.init_particles_using_file = 1

#particle intrinsics
bdem.normal_spring_constant = 5e4
bdem.normal_restitution_coefficient = 0.9
bdem.friction_coefficient = 0.0
bdem.tangential_spring_constant = 0.0
bdem.tangential_restitution_coefficient = 0.0
bdem.rolling_friction_coefficient = 0.0

bdem.gravity = 0.0 -9.8 0.0

bdem.stl_file=block.stl
